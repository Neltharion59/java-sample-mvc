' LoneClass.I()

@startuml

activate LoneClass

alt x > 8
    LoneClass -> FriendlyClass : Method01()
    activate FriendlyClass
else else
    LoneClass -> FriendlyClass : Method03()
    FriendlyClass -> DistantClass : MethodX()
    activate DistantClass
    deactivate DistantClass
end

deactivate FriendlyClass
deactivate LoneClass

@enduml